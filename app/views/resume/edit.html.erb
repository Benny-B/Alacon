<% provide(:title, "Resume") %>
<body>
<div class="container" id="contain"><%= form_for(@user, :id=> "mform") do |f| %>
  <h1> Upload your resume </h1>
	<div class="row" >
		<div class="span6 offset3">
 			 <div class="field">
           <%= f.file_field :resume %>
  			</div>
  			<%= f.submit "Submit", class: "btn btn-large btn-primary" %>
  		</div>
  	</div>
  	<% end %>
  </div>
</body>
